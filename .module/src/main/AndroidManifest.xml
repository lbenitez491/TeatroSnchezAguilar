                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="lb.app.teatrosnchezaguilar.module.quienes_somos"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="lb.app.teatrosnchezaguilar.module.ListadoActivity"
            android:label="@string/title_activity_listado" >
        </activity>
    </application>

</manifest>
